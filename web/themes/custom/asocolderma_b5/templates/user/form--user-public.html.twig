{#
/**
 * @file
 * Theme override for a 'form' element.
 *
 * Available variables
 * - attributes: A list of HTML attributes for the wrapper element.
 * - children: The child elements of the form.
 *
 * @see template_preprocess_form()
 */
#}
<div class="pre-form">
  <div class="pre-form__top">
    {{ drupal_block('text_resize_block') }}
    <div class="pre-form__status">
        <span>Estado de afiliación:</span>
        <span class="text">NO VERIFICADO</span>
        <a class="btn btn--primary" href="/usuario/estado-afiliacion">Resumen</a>
    </div>
  </div>
</div>
<div class="pre-form__links">
  <a class="btn btn--primary" href="/usuario/estadisticas">Estadísticas de mi perfil</a>
  {{ user_link }}
</div>
<div class="form-information">
    <h1>Actualizar datos públicos</h1>
    <p class="views">Total visitas a mi perfil:<strong>0</strong></p>
</div>
<form{{ attributes }}>
  {{ children }}
</form>
